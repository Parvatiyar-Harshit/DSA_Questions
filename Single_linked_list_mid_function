public static void mid(node head){
        node ptr1 = head;
        if(ptr1 == null){System.out.println(" ");}
        else{
            node ptr2  =head.next;
            while(ptr2!=null){
                ptr2 = ptr2.next;
                ptr1 = ptr1.next;
                if(ptr2 != null){ptr2 = ptr2.next;}
            }
            System.out.println(ptr1.data);
            
        }
    }
